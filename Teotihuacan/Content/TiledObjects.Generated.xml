<?xml version="1.0" encoding="utf-8"?>
<objecttypes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <objecttype name="Bullet">
    <property name="TeamIndex" type="int" />
    <property name="BulletSpeed" type="float" default="400" />
  </objecttype>
  <objecttype name="Enemy">
    <property name="SpriteAnimations" type="AnimationChainList" default="Enemy_Shooter_Animations" />
    <property name="MaxHp" type="int" />
    <property name="CurrentDataCategoryState" type="DataCategory" />
    <property name="FireShotsPerSecond" type="float" default="1" />
    <property name="MaxShootingDistance" type="float" default="200" />
  </objecttype>
  <objecttype name="Player">
    <property name="FireShotsPerSecond" type="float" default="4" />
  </objecttype>
  <objecttype name="SpawnPoint" />
</objecttypes>